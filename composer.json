{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "cocur/slugify": "^4.0",
        "easycorp/easyadmin-bundle": "^2.3",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "fzaninotto/faker": "^1.9",
        "giggsey/libphonenumber-for-php": "^8.12",
        "knplabs/knp-paginator-bundle": "^5.2",
        "lcobucci/jwt": "^3.3",
        "liip/imagine-bundle": "^2.3",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^5.2",
        "snilius/twig-sort-by-field": "^0.1.5",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.0.*",
        "symfony/google-mailer": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/mercure": "^0.4.0",
        "symfony/mercure-bundle": "^0.2.4",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/notifier": "5.2.*",
        "symfony/orm-pack": "2.1.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/sendgrid-mailer": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/string": "5.2.*",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/translation": "5.2.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/yaml": "5.2.*",
        "symfonycasts/verify-email-bundle": "^1.2",
       
        "twig/cssinliner-extra": "^3.0",
        "twig/inky-extra": "^3.0",
        "vich/uploader-bundle": "^1.14"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "liip/test-fixtures-bundle": "^1.7",
        "symfony/browser-kit": "5.2.*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.2.*"
        }
    },
    "name": "generation/boomerang",
    "description": ""
}
